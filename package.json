{
  "name": "ionic-hello-world",
  "author": "Ionic Framework",
  "homepage": "http://ionicframework.com/",
  "private": true,
  "scripts": {
    "api": "cd api && meteor run --settings private/settings.json",
    "lint:meteor": "tslint --format stylish --config ./tslint.json ./api/**/*.ts",
    "lint:ionic": "ionic-app-scripts lint",
    "lint": "npm run lint:meteor && lint:ionic",
    "ionic:build": "ionic-app-scripts build",
    "ionic:serve": "ionic-app-scripts serve",
    "meteor-client:bundle": "node meteor-client/bundler.js",
    "test:ionic": "jest"
  },
  "dependencies": {
    "@angular/common": "2.4.4",
    "@angular/compiler": "2.4.4",
    "@angular/compiler-cli": "2.4.4",
    "@angular/core": "2.4.4",
    "@angular/forms": "2.4.4",
    "@angular/http": "2.4.4",
    "@angular/platform-browser": "2.4.4",
    "@angular/platform-browser-dynamic": "2.4.4",
    "@angular/platform-server": "2.4.4",
    "@ionic/storage": "1.1.7",
    "@types/jest": "^16.0.4",
    "angular2-google-maps": "0.17.0",
    "angular2-moment": "1.1.0",
    "babel-runtime": "6.22.0",
    "ionic-angular": "2.0.0-rc.5",
    "ionic-native": "2.3.1",
    "ionicons": "3.0.0",
    "meteor-node-stubs": "0.2.4",
    "meteor-rxjs": "0.4.7",
    "meteor-typings": "1.3.1",
    "rxjs": "5.0.3",
    "sharp": "0.17.1",
    "sw-toolbox": "3.4.0",
    "zone.js": "0.7.6"
  },
  "devDependencies": {
    "@ionic/app-scripts": "1.0.0",
    "@types/meteor": "1.3.31",
    "@types/meteor-accounts-phone": "0.0.5",
    "@types/meteor-publish-composite": "0.0.32",
    "@types/underscore": "1.7.36",
    "angular2-template-loader": "^0.6.0",
    "jest": "^18.1.0",
    "jsesc": "^2.4.0",
    "mock-local-storage": "^1.0.2",
    "raf": "^3.3.0",
    "typescript": "2.1.5",
    "typescript-extends": "1.0.1"
  },
  "cordovaPlugins": [
    "cordova-plugin-camera",
    "cordova-plugin-console",
    "cordova-plugin-contacts",
    "cordova-plugin-device",
    "cordova-plugin-geolocation",
    "cordova-plugin-sim",
    "cordova-plugin-splashscreen",
    "cordova-plugin-statusbar",
    "cordova-plugin-whitelist",
    "https://github.com/Telerik-Verified-Plugins/ImagePicker",
    "ionic-plugin-keyboard"
  ],
  "cordovaPlatforms": [],
  "description": "Ionic2CLI-Meteor-Whatsapp: An Ionic project",
  "config": {
    "ionic_webpack": "./webpack.config.js"
  },
  "jest": {
    "verbose": true,
    "collectCoverage": false,
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "html",
      "css",
      ""
    ],
    "setupTestFrameworkScriptFile": "<rootDir>/test/config/init.ts",
    "testRegex": "/__tests__/.*\\.(ts|tsx|js)$",
    "transform": {
      "^.+\\.(ts|tsx|html)$": "<rootDir>/test/config/preprocessor.js"
    },
    "moduleNameMapper": {
      "^api/(.*)": "<rootDir>/api/server/$1"
    }
  }
}
